{
  "name": "@material-kit/lib",
  "version": "0.3.1",
  "description": "SCSS library that complements MDC-Web with useful components and utilities",
  "keywords": [
    "material-kit",
    "material",
    "material-design",
    "material-components",
    "web",
    "css",
    "scss"
  ],
  "homepage": "https://github.com/webdenim/material-kit",
  "bugs": {
    "url": "https://github.com/webdenim/material-kit/issues"
  },
  "license": "MIT",
  "author": "Webdenim",
  "files": [
    "components/**/*.scss"
  ],
  "main": "material-kit.scss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdenim/material-kit.git"
  },
  "scripts": {
    "clean": "del-cli _site/**",
    "dev": "npm run clean && parallelshell \"npm run docs:webpack\" \"npm run docs:jekyll\"",
    "docs:build": "npm run clean && webpack && cross-env JEKYLL_ENV=production bundler exec jekyll build --config _config.yaml,_prod-config.yaml",
    "docs:fix": "stylelint --fix _sass/**/*.scss",
    "docs:jekyll": "bundler exec jekyll serve",
    "docs:lint": "stylelint _sass/**/*.scss",
    "docs:publish": "push-dir --dir=_site --branch=gh-pages --cleanup",
    "docs:webpack": "webpack --watch",
    "fix": "stylelint --fix components/**/*.scss",
    "lint": "stylelint components/**/*.scss",
    "pretest": "npm run lint",
    "release": "standard-version --tag-prefix",
    "start": "npm run dev",
    "test": "echo TODO: make tests"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "del-cli": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "node-sass": "^4.5.3",
    "parallelshell": "^3.0.2",
    "push-dir": "^0.4.1",
    "sass-loader": "^6.0.6",
    "standard-version": "^4.2.0",
    "stylelint-config-material-kit": "^0.1.4",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "material-components-web": "^0.24.0",
    "reset-css": "^2.2.1"
  },
  "peerDependencies": {
    "material-components-web": "^0.24.0"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ]
  }
}
